{
  "name": "projectsite",
  "version": "1.0.0",
  "description": "Projectsite where we describe the vision and the goals of the impAct project to be visible and to get in contact with organizations and sponsors.",
  "author": "impact",
  "private": false,
  "license": "GPL-3.0-only",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "scully": "scully --noPrompt",
    "scully:gen": "npm run build && scully",
    "scully:serve": "scully serve",
    "scully:gen:serve": "npm run scully:gen && npm run scully:serve",
    "test": "ng test",
    "test:cov": "jest --coverage",
    "lint": "ng lint",
    "eslint": "eslint \"src/**/*.ts\"",
    "eslint:fix": "eslint \"src/**/*.ts\" --fix",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "e2e": "ng e2e",
    "pre-commit": "npm run prettier:fix && npm run eslint:fix",
    "commit": "npm run pre-commit && git add . && npx git-cz"
  },
